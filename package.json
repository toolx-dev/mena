{
  "devDependencies": {
    "standard": "^17.1.0",
    "standard-version": "^9.5.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "name": "mena",
  "version": "1.0.2",
  "description": "A customizable progress bar for console applications.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "watch": "tsc --watch",
    "build": "tsc -p tsconfig.json",
    "test": "vitest run",
    "test:watch": "vitest",
