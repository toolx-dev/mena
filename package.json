    "release:soft": "standard-version --skip.bump --skip.commit --skip.tag"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toolx-dev/mena.git"
  },
  "keywords": [
    "CLI",
    "progressbar",
    "style",
    "console",
    "progress",
    "bar"
  ],
  "author": "William Manco",
